        <result column="updated_at" property="updatedAt" />
        <result column="is_del" property="isDel" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        client_id, topic, status, created_at, updated_at, is_del
    </sql>

</mapper>
